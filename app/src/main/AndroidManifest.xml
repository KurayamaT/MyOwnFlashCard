<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.minpro.myownflashcard">

    <application
        android:name="net.minpro.myownflashcard.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.minpro.myownflashcard.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.minpro.myownflashcard.WordListActivity" />
        <activity android:name="net.minpro.myownflashcard.EditActivity" />
        <activity android:name="net.minpro.myownflashcard.TestActivity" />

    </application>

</manifest>